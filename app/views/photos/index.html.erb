
<table class="table table-striped table-condensed" style="width:50%;">
  <thead>
    <tr>
      <td>照片標題~!</td>
      <td>點選看看!!</td>
      <td>編輯照片</td>
      <td>刪除照片</td>
    </tr>
  </thead>
  <tbody>
 <% @photos.each do |photo| %>
   <tr>
     <td><%= photo.title %></td>
     <td><%= link_to 'Show', photo_path(photo) %></td>
     <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
     <td><%= link_to 'Destroy', photo_path(photo), method: :delete, data:{ confirm: "Are you sure?" } %></td>
   </tr>
   <% end %>
  </tbody>
</table>

<%= link_to 'New Photo--建立新照片', new_photo_path %>